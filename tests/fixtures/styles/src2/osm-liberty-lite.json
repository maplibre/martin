{
  "glyphs": "https://orangemug.github.io/font-glyphs/glyphs/{fontstack}/{range}.pbf",
  "id": "osm-liberty",
  "layers": [
    {
      "id": "background",
      "paint": {
        "background-color": "rgb(239,239,239)"
      },
      "type": "background"
    },
    {
      "id": "natural_earth",
      "maxzoom": 6,
      "paint": {
        "raster-opacity": {
          "base": 1.5,
          "stops": [[0, 0.6], [6, 0.1]]
        }
      },
      "source": "natural_earth_shaded_relief",
      "type": "raster"
    },
    {
      "filter": ["all", ["==", "class", "river"], ["!=", "brunnel", "tunnel"]],
      "id": "waterway_river",
      "layout": {
        "line-cap": "round"
      },
      "paint": {
        "line-color": "#a0c8f0",
        "line-width": {
          "base": 1.2,
          "stops": [[11, 0.5], [20, 6]]
        }
      },
      "source": "openmaptiles",
      "source-layer": "waterway",
      "type": "line"
    },
    {
      "filter": ["all", ["!=", "brunnel", "tunnel"]],
      "id": "water",
      "paint": {
        "fill-color": "rgb(158,189,255)"
      },
      "source": "openmaptiles",
      "source-layer": "water",
      "type": "fill"
    },
    {
      "filter": ["all", ["==", "admin_level", 2], ["!has", "claimed_by"]],
      "id": "boundary_2_z0-4",
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "hsl(248, 1%, 41%)",
        "line-opacity": {
          "base": 1,
          "stops": [[0, 0.4], [4, 1]]
        },
        "line-width": {
          "base": 1,
          "stops": [[3, 1], [5, 1.2], [12, 3]]
        }
      },
      "source": "openmaptiles",
      "source-layer": "boundary",
      "type": "line"
    },
    {
      "filter": ["all", ["==", "$type", "LineString"]],
      "id": "water_name_line",
      "layout": {
        "symbol-placement": "line",
        "text-field": "{name}",
        "text-font": ["Roboto Regular"],
        "text-max-width": 5,
        "text-size": 12
      },
      "paint": {
        "text-color": "#5d60be",
        "text-halo-color": "rgba(255,255,255,0.7)",
        "text-halo-width": 1
      },
      "source": "openmaptiles",
      "source-layer": "waterway",
      "type": "symbol"
    },
    {
      "filter": ["all", ["==", "class", "state"]],
      "id": "state",
      "layout": {
        "text-field": "{name_en}",
        "text-font": ["Roboto Condensed Italic"],
        "text-size": {
          "stops": [[4, 11], [6, 15]]
        },
        "text-transform": "uppercase"
      },
      "maxzoom": 6,
      "paint": {
        "text-color": "#633",
        "text-halo-color": "rgba(255,255,255,0.7)",
        "text-halo-width": 1
      },
      "source": "openmaptiles",
      "source-layer": "place",
      "type": "symbol"
    },
    {
      "filter": ["all", [">=", "rank", 3], ["==", "class", "country"]],
      "id": "country_3",
      "layout": {
        "text-field": "{name_en}",
        "text-font": ["Roboto Condensed Italic"],
        "text-max-width": 6.25,
        "text-size": {
          "stops": [[3, 11], [7, 17]]
        },
        "text-transform": "none",
        "visibility": "visible"
      },
      "paint": {
        "text-color": "#334",
        "text-halo-blur": 1,
        "text-halo-color": "rgba(255,255,255,0.8)",
        "text-halo-width": 1
      },
      "source": "openmaptiles",
      "source-layer": "place",
      "type": "symbol"
    },
    {
      "filter": ["all", ["==", "rank", 2], ["==", "class", "country"]],
      "id": "country_2",
      "layout": {
        "text-field": "{name_en}",
        "text-font": ["Roboto Condensed Italic"],
        "text-max-width": 6.25,
        "text-size": {
          "stops": [[2, 11], [5, 17]]
        },
        "text-transform": "none"
      },
      "paint": {
        "text-color": "#334",
        "text-halo-blur": 1,
        "text-halo-color": "rgba(255,255,255,0.8)",
        "text-halo-width": 1
      },
      "source": "openmaptiles",
      "source-layer": "place",
      "type": "symbol"
    },
    {
      "filter": ["all", ["==", "rank", 1], ["==", "class", "country"]],
      "id": "country_1",
      "layout": {
        "text-field": "{name_en}",
        "text-font": ["Roboto Condensed Italic"],
        "text-max-width": 6.25,
        "text-size": {
          "stops": [[1, 11], [4, 17]]
        },
        "text-transform": "none"
      },
      "paint": {
        "text-color": "#334",
        "text-halo-blur": 1,
        "text-halo-color": "rgba(255,255,255,0.8)",
        "text-halo-width": 1
      },
      "source": "openmaptiles",
      "source-layer": "place",
      "type": "symbol"
    },
    {
      "filter": ["all", ["==", "class", "continent"]],
      "id": "continent",
      "layout": {
        "text-field": "{name_en}",
        "text-font": ["Roboto Condensed Italic"],
        "text-justify": "center",
        "text-size": 13,
        "text-transform": "uppercase"
      },
      "maxzoom": 1,
      "paint": {
        "text-color": "#633",
        "text-halo-color": "rgba(255,255,255,0.7)",
        "text-halo-width": 1
      },
      "source": "openmaptiles",
      "source-layer": "place",
      "type": "symbol"
    }
  ],
  "metadata": {
    "maputnik:license": "https://github.com/maputnik/osm-liberty/blob/gh-pages/LICENSE.md",
    "maputnik:renderer": "mbgljs",
    "openmaptiles:version": "3.x"
  },
  "name": "OSM Liberty",
  "sources": {
    "natural_earth_shaded_relief": {
      "maxzoom": 6,
      "tileSize": 256,
      "tiles": ["https://tiles.openfreemap.org/natural_earth/ne2sr/{z}/{x}/{y}.png"],
      "type": "raster"
    },
    "openmaptiles": {
      "type": "vector",
      "url": "https://tiles.openfreemap.org/planet"
    }
  },
  "sprite": "https://maputnik.github.io/osm-liberty/sprites/osm-liberty",
  "version": 8
}
